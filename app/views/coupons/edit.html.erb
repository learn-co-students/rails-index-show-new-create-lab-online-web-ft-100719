<h1>Edit Coupon Details</h1>

<%= form_tag coupons_path, method="put" do %>

    <%= label_tag "coupon_code", "Coupon Code" %>
    <%= text_field_tag "coupon[coupon_code]" , value=@coupon.coupon_code %> <br>

    
    <%= label_tag "store", "Store" %>
    <%= text_field_tag "coupon[store]", value=@coupon.store %><br>

    <%= submit_tag "Update Coupon" %>

<% end %>
<%= link_to 'Back', coupons_path%>